{
  "name": "wildguard-frontend",
  "version": "2.0.0",
  "private": true,
  "description": "WildGuard AI - Real-time wildlife trafficking detection frontend with 16-language multilingual support",
  "dependencies": {
    "@nivo/bar": "^0.84.0",
    "@nivo/core": "^0.84.0",
    "@nivo/heatmap": "^0.84.0",
    "@nivo/line": "^0.84.0",
    "@nivo/pie": "^0.84.0",
    "@nivo/radar": "^0.84.0",
    "@nivo/stream": "^0.84.0",
    "@supabase/supabase-js": "^2.50.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "PORT=3000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:prod": "PORT=3000 REACT_APP_ENV=production react-scripts start",
    "build:prod": "REACT_APP_ENV=production react-scripts build",
    "serve": "npx serve -s build -l 3000",
    "analyze": "npx source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write src/**/*.{js,jsx,css,md}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "source-map-explorer": "^2.5.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "wildlife",
    "conservation",
    "ai",
    "trafficking",
    "detection",
    "multilingual",
    "supabase",
    "react"
  ],
  "homepage": "https://wildguard-frontend.vercel.app",
  "repository": {
    "type": "git",
