# WildGuard AI Frontend Environment Configuration - Development
# SECURITY: Replace with your actual Supabase credentials

# Supabase Configuration - REPLACE WITH YOUR ACTUAL VALUES
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Backend API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_API_TIMEOUT=30000

# Application Configuration
REACT_APP_NAME=WildGuard AI
REACT_APP_VERSION=2.0.0-dev
REACT_APP_DESCRIPTION=Wildlife Protection Intelligence Platform

# Feature Flags
REACT_APP_ENABLE_MULTILINGUAL=true
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_EXPORTS=true
REACT_APP_DEBUG_MODE=true

# Data Configuration
REACT_APP_DEFAULT_REFRESH_INTERVAL=30000
REACT_APP_MAX_SEARCH_RESULTS=100
REACT_APP_DEFAULT_DATE_RANGE=7

# Security Configuration
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_LOG_LEVEL=debug

# Environment
NODE_ENV=development
REACT_APP_ENV=development

# Development Tools
REACT_APP_SHOW_DEV_TOOLS=true
GENERATE_SOURCEMAP=true
